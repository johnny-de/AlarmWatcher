{
  "name": "AlarmWatcher",
  "short_name": "AlarmWatcher",
  "description": "github.com/johnny-de/alarmwatcher/",
  "version": "1.0.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#ffffff",
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png"
    },
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "scope": "/",
  "related_applications": [],
  "prefer_related_applications": false,
  "id": "/",
  "screenshots": [
    {
      "src": "/screenshot-wide.png",
      "sizes": "2208x1242",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "/screenshot-portrait.png",
      "sizes": "1242x2208",
      "type": "image/png"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+alarmwatcher",
      "url": "/handle-protocol?url=%s"
    }
  ],
  "display_override": ["window-controls-overlay"]
}
